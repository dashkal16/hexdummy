{
  "schemaVersion": 1,
  "id": "{{ modid }}",
  "version": "${version}",
  "name": "{{ display_name }}",
  "description": "{{ description }}",
  "authors": [
    "{{ author }}"
  ],
  "contact": {
    "homepage": "{{ homepage_url }}",
    "sources": "{{ sources_url }}"
  },
  "license": "{{ license }}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "{{ package }}.fabric.{{ classname }}Fabric"
    ],
    "client": [
      "{{ package }}.fabric.{{ classname }}ClientFabric"
    ]
  },
  "mixins": [
    "{{ modid }}-common.mixins.json",
    "{{ modid }}.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraftVersion}",
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "architectury": ">=${architecturyVersion}",
    "hexcasting": ">=${hexcastingVersion}"
  }
}
